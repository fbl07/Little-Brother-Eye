</head>
<body>
    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Little Brother Eye", "Index", "Event", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Event List", "Index", "Event")</li>
                    <li>@Html.ActionLink("Developer", "Index", "developer")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        <div style="height:15px;"></div>
        @RenderBody()
        @*<hr />*@
        @*<footer class="footer">
            <hr />
            <p>&copy; @DateTime.Now.Year - Francis Lord</p>
        </footer>*@

        <div class="navbar navbar-default navbar-fixed-bottom">
            <div class="container">
                <div class="navbar-header">
                    @Html.ActionLink("Little Brother Eye", "Index", "Event", new { area = "" }, new { @class = "navbar-brand" })
                </div>
                <div class="navbar-collapse collapse">
                    <p class="navbar-text">&copy; @DateTime.Now.Year - Francis Lord</p>

                    <div class="navbar-left">
                        <form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top" style="margin-top:12px;">
                            <input type="hidden" name="cmd" value="_s-xclick">
                            <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----">
                            <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donate_LG.gif" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!">
                            <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">
                        </form>
                    </div>

                    @{ 
                        string version = "Version " + ViewBag.Version;
                    }

                    <p class="navbar-text navbar-right" style="margin-right:15px;">@Html.ActionLink(version, "ReleaseNotes", "Home")</p>
                </div>
            </div>
        </div>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Scripts/bootstrap-clockpicker.min.js")
    @*@Scripts.Render("~/Scripts/Custom/footer.js")*@
    @Scripts.Render("~/Scripts/moment.js")
    @RenderSection("scripts", required: false)
</body>
</html>
